cmd - h : yabai -m window --focus west
cmd - l : yabai -m window --focus east
cmd - k : yabai -m window --focus north
cmd - j : yabai -m window --focus south
shift + cmd - h : yabai -m window --swap west
shift + cmd - l : yabai -m window --swap east
shift + cmd - h : yabai -m window --swap north
shift + cmd - l : yabai -m window --swap south

ctrl + cmd - t : yabai -m window --toggle float;

# alt - 1 : yabai -m window --focus 1
# alt - 2 : yabai -m window --focus 2

shift + cmd - 0 : yabai -m space --balance

alt - z  : yabai -m display --focus next
shift + alt - z  : yabai -m display --swap next
